import React from "react";
function Card({image, title, price, description, button}){
    return(
        <div className="card">
            <div className="ca">
             <img src={image} height='150' width='150' alt={title}/>
             <h2>{title}</h2>
             <h3>{price}</h3>
             <p>{description}</p>
             <button>{button}</button>
            </div>
        </div>
    )

}
export default Card;